<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:color="#B5D4A7">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#B5D4A7"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#EDF5EA"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
</selector>